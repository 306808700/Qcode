<!doctype html>
<html>
<head>
    <meta charset="utf-8"/>
    <title>命令行编码器 - hcs</title>
    <script src="http://g.tbcdn.cn/kissy/k/1.3.0/kissy-min.js" charset="utf-8" class="hcs_script"></script>
    <script type="text/javascript" class="hcs_script">
		var S = KISSY;
		S.Config.debug = true;
		S.config({
			packages:[
			    {
			        name:"hcs",
			        path:"/",
			        charset:"utf-8"
			    }
			]
		});
		KISSY.use("hcs",function(S,HCS){
			var config = {
				path:"html/"
			};
			hcs = new HCS(config);
			hcs.init();
		});
		</script>
</head>
<body>
</body>
</html>